{
  "force": {
    "version": "0.0.0-development",
    "scripts": {
      "clean": "rm -rf lib",
      "build": "npx babel src --out-dir lib --source-maps inline --copy-files --include-dotfiles",
      "build-clean": "npm run clean && npm run build",
      "test-simple": "nyc mocha \"./test/**/*.spec.js\"",
      "test": "npm run clean && npm run gardener && npm run test-simple",
      "coveralls": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
      "semantic-release": "npm run build-clean && npx semantic-release"
    },
    "nyc": {
      "tempDir": "./coverage/.nyc_output",
      "report-dir": "./coverage",
      "check-coverage": true,
      "per-file": false,
      "lines": 100,
      "statements": 100,
      "functions": 100,
      "branches": 100,
      "include": [
        "**/*.js"
      ],
      "reporter": [
        "lcov",
        "text-summary"
      ],
      "require": [
        "@babel/register"
      ],
      "extension": [],
      "cache": true,
      "all": true,
      "babel": true
    }
  },
  "defaults": {
    "scripts": {
      "gardener": "node gardener"
    },
    "name": "???",
    "description": "???",
    "main": "lib/???.js",
    "keywords": [],
    "homepage": "{{GIT_URL}}#readme",
    "devDependencies": {
      "@babel/register": "7.0.0",
      "babel-eslint": "8.2.3",
      "@babel/core": "7.1.2",
      "@babel/cli": "7.1.2",
      "@babel/plugin-transform-flow-comments": "7.0.0",
      "@babel/plugin-proposal-object-rest-spread": "7.0.0",
      "@babel/preset-flow": "7.0.0",
      "coveralls": "3.0.0",
      "nyc": "12.0.2",
      "semantic-release": "15.7.1"
    },
    "engines": {
      "node": ">= 8.10"
    },
    "repository": {
      "type": "git",
      "url": "{{GIT_URL}}.git"
    },
    "bugs": {
      "url": "{{GIT_URL}}/issues"
    },
    "files": [
      "lib"
    ],
    "nyc": {
      "exclude": [
        "gardener.js",
        "node_modules/*",
        "coverage/*",
        "flow-typed/*",
        "lib/*"
      ]
    }
  }
}

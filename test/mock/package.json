{
  "name": "mock",
  "version": "0.0.0-development",
  "description": "One Paragraph of project description goes here",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npx babel src --out-dir lib --source-maps inline --copy-files --include-dotfiles",
    "clean-build": "npm run clean && npm run build",
    "test": "npm run clean && npm run gardener && npm run test-simple",
    "coveralls": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "build-artifact": "npm run clean-build && tar -zxvf $(npm pack) && rm *-0.0.0-development.tgz && cd package && tar -zcvf ../release.tar.gz . && cd .. && rm -rf package",
    "semantic-release": "npm run build-artifact && npm run clean-build && npx semantic-release",
    "gardener": "node gardener",
    "test-simple": "nyc mocha \"./test/**/*.spec.js\""
  },
  "author": "The Author",
  "license": "MIT",
  "devDependencies": {
    "chai": "4.1.2",
    "coveralls": "3.0.2",
    "semantic-release": "15.9.15",
    "nyc": "13.0.1",
    "babel-cli": "7.0.0-beta.3",
    "babel-plugin-transform-flow-comments": "7.0.0-beta.3",
    "babel-register": "7.0.0-beta.3",
    "babel-eslint": "9.0.0",
    "babel-preset-flow": "7.0.0-beta.3"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/blackflux/js-gardener/blob/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">= 8.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blackflux/js-gardener.git"
  },
  "bugs": {
    "url": "https://github.com/blackflux/js-gardener/issues"
  },
  "homepage": "https://github.com/blackflux/js-gardener",
  "files": [
    "lib"
  ],
  "dependencies": {},
  "peerDependencies": {},
  "keywords": [],
  "nyc": {
    "tempDirectory": "./coverage/.nyc_output",
    "report-dir": "./coverage",
    "check-coverage": true,
    "per-file": false,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "gardener.js",
      "node_modules/*",
      "coverage/*",
      "flow-typed/*",
      "lib/*"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "babel-register"
    ],
    "extension": [],
    "cache": true,
    "all": true,
    "babel": true
  }
}

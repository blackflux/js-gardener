{
  "npmPublish": false,
  "publish": [
    {
      "path": "@semantic-release/github",
      "assets": [
        {
          "path": "release.tar.gz",
          "label": "Release Artifact"
        }
      ]
    }
  ],
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {
          "type": "build",
          "scope": "deps",
          "release": "minor"
        }
      ]
    }
  }
}
